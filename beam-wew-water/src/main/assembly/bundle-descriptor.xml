<bundleDescriptor>
    <bundleName>${artifactId}</bundleName>
    <bundleVersion>${version}</bundleVersion>
    <processorDescriptors>
        <processorDescriptor>
            <executableName>FUB.Water</executableName>
            <processorName>FUB/WeW Water</processorName>
            <processorVersion>${version}</processorVersion>
            <defaultParameters>
                <![CDATA[
<parameters>
    <computeCHL>true</computeCHL>
    <computeYS>true</computeYS>
    <computeTSM>true</computeTSM>
    <computeAtmCorr>true</computeAtmCorr>
    <checkWhetherSuspectIsValid>true</checkWhetherSuspectIsValid>
    <expression>not l1_flags.GLINT_RISK and not l1_flags.BRIGHT and not l1_flags.INVALID and not l1_flags.SUSPECT</expression>
</parameters>]]>
            </defaultParameters>
            <outputFormats>BEAM-DIMAP,NetCDF,GeoTIFF</outputFormats>
            <descriptionHtml>
                <![CDATA[<p>Retrieves case II water properties and atmospheric properties above case II waters.</p>]]></descriptionHtml>
            <inputProductTypes>MERIS_L1B</inputProductTypes>
            <outputProductType>MERIS_FUB_WEW</outputProductType>
            <validMaskExpression>!l1_flags.INVALID AND !l1_flags.BRIGHT AND !l1_flags.GLINT_RISK AND !l1_flags.SUSPECT</validMaskExpression>
            <outputVariables>
                <outputVariable>
                    <name>algal_2</name>
                </outputVariable>
				<outputVariable>
                    <name>yellow_subs</name>
                </outputVariable>
				<outputVariable>
                    <name>total_susp</name>
                </outputVariable>
				<outputVariable>
                    <name>aero_opt_thick_440</name>
                </outputVariable>
				<outputVariable>
                    <name>aero_opt_thick_550</name>
                </outputVariable>
				<outputVariable>
                    <name>aero_opt_thick_670</name>
                </outputVariable>
				<outputVariable>
                    <name>aero_opt_thick_870</name>
                </outputVariable>
				<outputVariable>
                    <name>reflec_1</name>
                </outputVariable>
				<outputVariable>
                    <name>reflec_2</name>
                </outputVariable>
				<outputVariable>
                    <name>reflec_3</name>
                </outputVariable>
				<outputVariable>
                    <name>reflec_4</name>
                </outputVariable>
				<outputVariable>
                    <name>reflec_5</name>
                </outputVariable>
				<outputVariable>
                    <name>reflec_6</name>
                </outputVariable>
				<outputVariable>
                    <name>reflec_7</name>
                </outputVariable>
				<outputVariable>
                    <name>reflec_9</name>
                </outputVariable>
				<outputVariable>
                    <name>l1_flags</name>
                </outputVariable>
                <outputVariable>
                    <name>result_flags</name>
                </outputVariable>
            </outputVariables>
            <jobConfig>
                <jobParameter>
                    <name>calvalus.calvalus.bundle</name>
                    <value>calvalus-2.7</value>
                </jobParameter>
                <jobParameter>
                    <name>calvalus.snap.bundle</name>
                    <value>beam-5.0.1-portal</value>
                </jobParameter>
                <jobParameter>
                    <name>calvalus.system.beam.useAlternatePixelGeoCoding</name>
                    <value>true</value>
                </jobParameter>
            </jobConfig>
        </processorDescriptor>
    </processorDescriptors>
</bundleDescriptor>